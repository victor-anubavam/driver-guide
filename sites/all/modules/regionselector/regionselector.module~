function ep($obj) {
	echo '<pre>'; print_r($obj);
}
function epe($obj) {
	ep($obj); exit;
}


function regionselector_form_alter(&$form, &$form_state, $form_id)
{
	print '<pre>';
	print_r($form);
	print '</pre>';
		
}
?>
